package git.part1;

import java.lang.reflect.Array;
import java.util.ArrayList;
import java.util.Arrays;
import java.util.Collections;
import java.util.LinkedHashSet;
import java.util.Random;
import java.util.Set;

public class lotto 
{
	
	public static void main(String[] args)
	{
		 int[] lottery = new int[6];
		    int randomNum;

		    for (int i = 0; i < 5; i++) {
		        randomNum = (int) (Math.random() * 59); // Random number created here.
		        for (int x = 0; x < i; x++) {
		            if (lottery[x] == randomNum) // Here, code checks if same random number generated before.
		            {
		                randomNum = (int) (Math.random() * 59);// If random number is same, another number generated.
		                x = -1; // restart the loop
		            }

		        }
		        lottery[i] = randomNum;
		    }
		 

		    for (int i = 0; i < 1; i++) {
		        randomNum = (int) (Math.random() * 35); // Random number created here.
		        for (int x = 0; x < i; x++) {
		            if (lottery[x] == randomNum) // Here, code checks if same random number generated before.
		            {
		                randomNum = (int) (Math.random() * 35);// If random number is same, another number generated.
		                x = -1; // restart the loop
		            }

		        }
		        lottery[6] = randomNum;
		    }
		    for (int i = 0; i < lottery.length; i++)
		        System.out.print(lottery[i] + " ");
		
		//--------------------
		

	
}}


